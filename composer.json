{
    "name": "nailsapp/module-cdn",
    "keywords": ["Nails", "Shed Collective", "CodeIgniter", "CDN Module"],
    "homepage": "http://nailsapp.co.uk/modules/cdn",
    "description": "This is the \"CDN\" module for Nails.",
    "license": "MIT",
    "authors":
    [
        {
            "name": "Pablo de la Pena",
            "email": "p@shedcollective.org",
            "homepage": "http://shedcollective.org",
            "role": "Developer"
        },
        {
            "name": "Gary Duncan",
            "email": "g@shedcollective.org",
            "homepage": "http://shedcollective.org",
            "role": "Developer"
        }
    ],
    "support":
    {
        "email": "hello@shedcollective.org"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/hellopablo/PHPThumb"
    }],
    "require":
    {
        "nailsapp/common": "dev-develop",
        "nailsapp/driver-cdn-local": "dev-develop",
        "sybio/gif-creator": "~1.0.0",
        "sybio/gif-frame-extractor": "~1.0.0",
        "masterexploder/phpthumb": "dev-master"
    },
    "require-dev":
    {
        "phpunit/phpunit": "4.*"
    },
    "autoload":
    {
        "psr-4": {"Nails\\Cdn\\": "src/"}
    },
    "extra":
    {
        "nails" :
        {
            "moduleName": "cdn",
            "type": "module",
            "autoload": {
                "helpers": ["cdn"]
            }
        }
    }
}
